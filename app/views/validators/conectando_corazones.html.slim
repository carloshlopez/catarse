          = render partial: 'projects/project', locals: {project: @proj_bog, custom_class: 'first' }
      br
      .selected_projects.list
        .list_title
          .title
            h2  Resto del país
        .projects
          = render partial: 'projects/project', locals: {project: @proj_neiva, custom_class: 'first' }
          = render partial: 'projects/project', locals: {project: @proj_cucuta, custom_class: 'first' }
          = render partial: 'projects/project', locals: {project: @proj_buc, custom_class: 'first' }
    .clearfix   
