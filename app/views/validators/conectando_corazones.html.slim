      .section_header
        .original_title
          p
            = image_tag @campaign.logo_url
          p
            = "Una campaña de"
          p
            =image_tag @validator.logo_url, :width=>"250px"
      .selected_projects.list
        .list_title
          .title
            h2 Región Andina
        .projects
          = render partial: 'projects/project', locals: {project: @proj_med, custom_class: 'first' }
